<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fisli.basketbuddy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Home">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ItemChecklist" />
        <activity
            android:name=".ItemEdit"
            android:label="@string/title_activity_item_edit"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".TripSummary" />
        <activity android:name=".Receipt" />
        <activity android:name=".TripHistory" />
        <activity android:name=".AvgSpending" />
        <activity android:name=".EditStore" />
        <activity android:name=".NewTrip" />
        <activity android:name=".TripMenu"></activity>
    </application>

</manifest>